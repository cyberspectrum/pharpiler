{
  "name": "cyberspectrum/pharpiler",
  "description": "Creates a phar from any composer based project - highly configurable",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "ext-pcre": "*",
    "psr/log": "^1.0",
    "symfony/config": "^5.0",
    "symfony/console": "^5.0",
    "symfony/finder": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/process": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "ext-Phar": "*",
    "ext-bz2": "*",
    "ext-zlib": "*"
  },
  "autoload": {
    "psr-4": {
      "CyberSpectrum\\PharPiler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CyberSpectrum\\PharPiler\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "bin": ["bin/pharpiler"]
}
